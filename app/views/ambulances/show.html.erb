  <div class="container">
    <div class="big-shop">
      <h1><%= @ambulance.shop_num %></h1>
    </div>
    <div class="img-wrap">
      <div class=show-img>
        <%= ambulance_picture %>
      </div>
      <p>&copy;<%= @ambulance.show_img_cap %></p>
    </div>
    <div class="make-model">
      <p><%= @ambulance.year %> <%= @ambulance.make_model %></p>
    </div>
    <hr>
    <div class="assignments">
      <h3>Assignments</h3>
      <div class="col-md-6 left-info">
        <p><%= @ambulance.original_assign %></p>
        <p><%= @ambulance.second_assign %></p>
        <p><%= @ambulance.third_assign %></p>
        <p><%= @ambulance.fourth_assign %></p>
        <p><%= @ambulance.fifth_assign %></p>
        <p><%= @ambulance.sixth_assign %></p>
      </div>
      <div class="col-md-6 right-info">
        <p><%= @ambulance.seventh_assign %></p>
        <p><%= @ambulance.eighth_assign %></p>
        <p><%= @ambulance.ninth_assign %></p>
        <p><%= @ambulance.tenth_assign %></p>
        <p><%= @ambulance.eleventh_assign %></p>
      </div>
    </div>
    <div class="col-md-12 notes">
      <h3>Vehicle Notes</h3>
      <p><%= @ambulance.notes %></p>
    </div>
    <div class="col-md-12 img-links">
      <h3 class="">Additional Images</h3>
      <p><a href=<%= @ambulance.img_link %> target="_blank"><%= @ambulance.caption %></a>
        <a href=<%= @ambulance.img_link_two %> target="_blank"><%= @ambulance.caption_two %></a>
        <a href=<%= @ambulance.img_link_three %> target="_blank"><%= @ambulance.caption_three %></a>
        <a href=<%= @ambulance.img_link_four %> target="_blank"><%= @ambulance.caption_four %></a>
        <a href=<%= @ambulance.img_link_five %> target="_blank"><%= @ambulance.caption_five %></a>
      </p>
    </div>
    <hr>
  </div>
</body>