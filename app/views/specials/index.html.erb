                <a href=<%= special.img_link_seven %> target="_blank"><%= special.caption_seven %></a>
                <a href=<%= special.img_link_eight %> target="_blank"><%= special.caption_eight %></a>
                <a href=<%= special.img_link_nine %> target="_blank"><%= special.caption_nine %></a>
                <a href=<%= special.img_link_ten %> target="_blank"><%= special.caption_ten %></a>
                <a href=<%= special.img_link_eleven %> target="_blank"><%= special.caption_eleven %></a>
                <a href=<%= special.img_link_twelve %> target="_blank"><%= special.caption_twelve %></a>
                <a href=<%= special.img_link_thirteen %> target="_blank"><%= special.caption_thirteen %></a>
