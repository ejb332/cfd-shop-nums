  <div class="container">
    <div class="table-responsive">
      <table class="table table-stripe">
        <thead class="thead-inverse ">
          <tr class="row-head">
            <th scope="col" class="shop">Shop &#35</th>
            <th scope="col" class="year">Year</th>
            <th scope="col">Make&#47Model</th>
            <th scope="col">Original Assignment</th>
            <th scope="col">Second Assignment</th>
            <th scope="col">Third Assignment</th>
            <th scope="col">Photos</th>
          </tr>
        </thead>
        <tbody>
          <% @specials.each do |special| %>
            <tr>
              <th scope="row" class="shop"><a href="/g-series/<%= special.id %>"><%= special.shop_num %></a></a></th>
              <td class="year"><%= special.year %></td>
              <td><%= special.make_model %></td>
              <td><%= special.original_assign %></td>
              <td><%= special.second_assign %></td>
              <td><%= special.third_assign %></td>
              <td><a href=<%= special.img_link %> target="_blank"><%= special.caption %></a>
                <a href=<%= special.img_link_two %> target="_blank"><%= special.caption_two %></a>
                <a href=<%= special.img_link_three %> target="_blank"><%= special.caption_three %></a>
                <a href=<%= special.img_link_four %> target="_blank"><%= special.caption_four %></a>
